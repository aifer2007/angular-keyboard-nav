{
  "name": "angular-keyboard-nav-app",
  "version": "1.0.2",
  "description": "Angular directive to navigate elements with keyboard arrow keys.",
  "keywords": [
    "angular",
    "keyboard",
    "navigation",
    "arrow-keys"
  ],
  "author": {
    "name": "Justin Lettau",
    "email": "me@justinlettau.com",
    "url": "http://justinlettau.com"
  },
  "license": "MIT",
  "homepage": "https://justinlettau.github.io/angular-keyboard-nav/",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinlettau/angular-keyboard-nav"
  },
  "bugs": {
    "url": "https://github.com/justinlettau/angular-keyboard-nav/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "npm run build:lib && npm run build:app",
    "build:app": "ng build --prod --base-href /angular-keyboard-nav/",
    "build:lib": "ng build angular-keyboard-nav --prod && node scripts/postbuild.js",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "npm run lint.app && npm run lint.sass && npm run lint.tools",
    "lint.app": "ng lint",
    "lint.sass": "sass-lint -c .sass-lint.yml -v",
    "lint.tools": "eslint e2e/**/*.js projects/**/*.js scripts/**/*.js src/**/*.js -c .eslintrc.json",
    "release": "standard-version",
    "prepublishOnly": "npm run build:lib",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist/angular-keyboard-nav-app"
  },
  "dependencies": {
    "@angular/animations": "^7.0.2",
    "@angular/common": "^7.0.2",
    "@angular/compiler": "^7.0.2",
    "@angular/core": "^7.0.2",
    "@angular/forms": "^7.0.2",
    "@angular/http": "^7.0.2",
    "@angular/platform-browser": "^7.0.2",
    "@angular/platform-browser-dynamic": "^7.0.2",
    "@angular/router": "^7.0.2",
    "core-js": "^3.0.0",
    "rxjs": "^6.2.2",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "^0.13.0",
    "@angular/cli": "^8.0.0",
    "@angular/compiler-cli": "^7.0.2",
    "@angular/language-service": "^7.0.2",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.12.1",
    "bootstrap": "^4.1.3",
    "codelyzer": "^4.4.2",
    "eslint": "^5.2.0",
    "eslint-config-unicorn-science": "^1.1.0",
    "fs-extra": "^7.0.0",
    "gh-pages": "^2.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~2.0.0",
    "karma-jasmine-html-reporter": "^1.2.0",
    "ng-packagr": "^4.0.0",
    "protractor": "~5.4.0",
    "sass-lint": "^1.12.1",
    "sass-lint-config-unicorn-science": "^1.1.0",
    "standard-version": "^5.0.0",
    "ts-node": "~8.1.0",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.3",
    "tslint": "~5.16.0",
    "tslint-config-unicorn-science": "^1.3.0",
    "typescript": "~3.1.5"
  }
}
